<launch>
    <arg name="model" default="burger"/>
    <arg name="x_pos" default="2.0"/>
    <arg name="y_pos" default="0.0"/>

    <include file="$(find turtlebot3_gazebo)/launch/turtlebot3_empty_world.launch">
        <arg name="model" default="$(arg model)"/>
        <arg name="x_pos" default="$(arg x_pos)"/>
        <arg name="y_pos" default="$(arg y_pos)"/>
    </include>

    <!-- node tag for linear generating function -->
    <!-- <node pkg="turtlebot3_control" name="annular" type="annular" output="screen"/> -->
    <!-- node tag for quadratic generating function -->
    <node pkg="turtlebot3_control" name="quadratic" type="quadratic" output="screen"/>
    <!-- node tag for switching -->
    <!-- <node pkg="turtlebot3_control" name="switching" type="switching" output="screen"/> -->
    <!-- loading params -->
    <rosparam file="$(find turtlebot3_control)/config/params.yaml" />

</launch>